  "devDependencies": {
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "react": "^19.0.1",
    "react-dom": "^19.0.1",
    "tsup": "^8.3.0",
    "typescript": "^5.7.3"
  },
  "keywords": [
    "react",
    "ui",
    "components",
    "typescript",
    "tailwind"
